/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
page {
  height: 100%;
  background-color: #F8F8F8;
  font-size: 14px;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}